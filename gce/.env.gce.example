# .env.gce.example
# Greenbone Community Edition Environment Variables
# Copy this file to .env.gce and configure

# PostgreSQL configuration
GCE_POSTGRES_DB=gvmd
GCE_POSTGRES_USER=gce
GCE_POSTGRES_PASSWORD=gce_password_123

# GCE Admin credentials
GCE_ADMIN_USER=admin
GCE_ADMIN_PASSWORD=admin_password_123

# Port configuration
# Web UI port (default 8443 to avoid conflicts with standard HTTPS)
GCE_WEB_PORT=8443
# API port
GCE_API_PORT=9390

# Optional: Disable Notus scanner
GCE_DISABLE_NOTUS=false